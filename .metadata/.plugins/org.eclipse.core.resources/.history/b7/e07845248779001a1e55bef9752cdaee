package com.in28minutes.oops;

public class MyNumber {

	private int number;

	public MyNumber(int number) {
		this.number = number;
	}


	public boolean isPrime() {
		for (int i = 2; i <= number - 1; i++) {
			if (number % i == 0) {
				return false;
			}
		}
		return true;
	}

	public int sumUpToN() {
		int sum = 0;
		for (int i = 1; i <= number; i++) {
			sum = sum + i;
		}

	}
}
